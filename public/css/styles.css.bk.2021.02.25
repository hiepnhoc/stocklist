	/*---======= BG TITLE STOCH HEADER =======---*/
	.main-content .nav-title-stock {
	    padding: 5px 0 0;
	    z-index: 54;
	    position: relative;
	    background: #232323; 
	    transition: all 0.3s ease 0s;
	}

		.nav-title-stock .search-box {
		    display: block;
		    float: left;
		    font-size: 0;  
		}

			.search-box .search-box-main {
			    float: left;
			    font-size: 13px;
			    width: 100px; 
			}

				.search-box .search-box-main input {
				    background-color: #ccc;
				    border: 0;
				    border-radius: 15px 0 0 15px;
				    box-sizing: border-box;
				    color: #666;
				    display: inline-block;
				    font-family: Arial,Sans-Serif;
				    font-size: 13px;
				    height: 25px;
				    line-height: 25px;
				    padding: 0 5px;
				    position: relative;
				    vertical-align: top;
				    width: 105px; 
				    border: 1px solid #cfe54c;
				    transition: all .2s ease;
				}

				.search-box .search-box-main input:focus {
				    background: #fff;
				    box-shadow: 0 0 5px hsla(0,0%,100%,.5);
				    color: #666;
				}

				.search-box button {
				    border-radius: 100%;
				    display: inline-block;
				    height: 25px;
				    width: 25px;
				    line-height: 20px;
				    padding: 0;
				    border: 0;
				    color: #fff;
				    cursor: pointer;
				    background-color: #bfd630;
				    text-align: center;
				    vertical-align: top;
				    font-size: 12px;
				    transition: all .2s ease;
				    outline-style: none;
				    position: relative;
				    box-shadow: 1px 3px 10px rgba(0,0,0,.3);
				    color: #fff; 
				    left: -5px;
				    position: relative;
				}

				.search-box3 {
					width: 125px;
					position: relative;
				}

				/*.search-box3 button {
					position: absolute; 
				    right: 5px;
				}
				*/
				.search-box3 .search-box-main input {
					border-radius: 15px 3px 3px 15px;
				}

				/*.search-box2 .buttonchart  {
					left: -5px;
				}*/

				.button:hover { 
				    background-color: #3a3a3c;
				}  

		.nav-title-stock >ul{
		    list-style: none;
		    float: left;
		    font-size: 13px;
		    margin: 0;
		    padding: 0;
		}

			.nav-title-stock >ul>li{
			    display: inline-block;
			    float: left;
			    height: 30px;
			    margin-right: 4px;
			    position: relative;
			}

			.nav-title-stock >ul>li>a {
				background: #3a3a3a;
			    border-radius: 3px 3px 0 0;
			    display: inline-block;
			    height: 30px;
			    color: #f0f0f0;
			    line-height: 30px;
			    max-width: 180px;
			    /*overflow: hidden;*/
			    display: block;
			    padding: 0 15px;
			    text-overflow: ellipsis;
			    transition: background-color .1s;
			    white-space: nowrap;
			}

			.nav-title-stock >ul>li:hover>a {
			    background: #4a4a4a;
			} 

			.nav-title-stock >ul>li.has-sub-menu>a:after {
			    display: inline-block;
			    font: normal normal normal 14px/1 "FontAwesome";
			    font-size: inherit;
			    text-rendering: auto;
			    -webkit-font-smoothing: antialiased;
			    -moz-osx-font-smoothing: grayscale;
			    content: "\F0D7";
			    margin-left: 8px;
			}
			
			.hideinforchart , .hidechart {
				border-radius: 100%;
			    width: 25px;
			    height: 25px;
			    text-align: center;
			    line-height: 25px; 
			    background: #3a3a3a;
			    display: block;
			    float: left;
			    margin-left: 5px;
			}  

			.activeicon{ background: #cfe44b !important; color: #0902ff;}

			/*----- SUB MENU ------*/
				.nav-title-stock >ul>li ul.sub-menu {
				    background: rgba(58,58,58,.97);
				    border-radius: 0 0 5px 5px;
				    border-top: 2px solid #bfd630;
				    border-left: 1px solid #444;
				    border-right: 1px solid #444;
				    border-bottom: 1px solid #444;
				    box-shadow: 3px 5px 20px rgba(0,0,0,.5);
				    display: none;
				    font-size: 95%;
				    height: auto;
				    left: 0;
				    list-style: none;
				    max-height: 290px;
				    overflow: auto;
				    padding: 10px 8px 8px;
				    position: absolute;
				    top: 30px;
				    transition: background-color .2s;
				    width: 20em;
				    z-index: 52;
				}

				.nav-title-stock >ul>li ul.sub-menu li{
				    border-radius: 3px;
				    overflow: hidden;
				    position: relative;
				}

				.nav-title-stock  >ul>li ul.sub-menu li a {
				    display: block;
				    line-height: 1.3;
				    padding: 6px 8px;
				    color: #ddd;
				    transition: background-color .1s,color .1s,box-shadow .2s;
				}

				.nav-title-stock >ul>li ul.sub-menu li.selected{
				    background-color: rgba(191, 214, 48, 0.3);
				    color: #fff;
				}

				.nav-title-stock >ul>li ul.sub-menu li a:hover {
				    background: #4a4a4a;
				    color: #fff;
				}

				.nav-title-stock >ul>li:hover ul.sub-menu {
				    display: block;
				}

				.nav-title-stock >ul>li ul.sub-menu.two-column li {
				    float: left;
				    width: 50%;
				} 

				.nav-title-stock >ul>li ul.sub-menu.two-column {
				    width: 32.5em;
				}

					.nav-title-stock >ul>li ul.sub-menu li.add {
					    background-color: transparent;
					    margin: 2px 0 0;
					}

					.nav-title-stock >ul>li ul.sub-menu li input {
					    background: rgba(0,0,0,.2);
					    border: 1px solid hsla(0,0%,100%,.2);
					    border-radius: 3px;
					    color: #fff;
					    font-family: Arial,Sans-Serif;
					    padding: 4px;
					    margin: 2px;
					    width: 14em;
					    transition: box-shadow .2s;
					}

					.nav-title-stock >ul>li ul.sub-menu li input:focus{
					    box-shadow: 0 0 10px rgba(247,148,29,.7);
					}

					.nav-title-stock>ul>li ul.sub-menu li .editing-buttons{
					    position: absolute;
					    right: 5px;
					    top: 3px;
					}

					.nav-title-stock >ul>li ul.sub-menu li .editing-buttons button{
					    background: none;
					    border: none;
					    border-radius: 3px;
					    box-sizing: border-box;
					    color: hsla(0,0%,100%,.7);
					    font-size: 125%;
					    height: 1.4em;
					    line-height: 1.4em;
					    padding: 0 4px;
					    transition: background-color .2s;
					}

					.nav-title-stock >ul>li ul.sub-menu li .editing-buttons button:hover {
					    background: hsla(0,0%,100%,.2);
					    color: #fff;
					}

			/*----- SUB MENU ------*/

		.nav-title-stock >ul.secondary {
		    float: left;
		    margin: 0;
		}

		.nav-title-stock >ul.secondary>li .sub-menu {
		    left: auto;
		    right: 0;
		    width: 140px;
		}

			.nav-title-stock >ul .secondary>li>a{
			    color: #ddd;
			    font-size: 13px;
			    padding-left: 11px;
			    padding-right: 11px;
			} 

			.nav-title-stock  .secondary>li>a >i{
				line-height: 30px;
			}

